import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] arr=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        int k=sc.nextInt();
        int ls=0,rs=0,maxs=0;
        for(int i=0;i<k;i++)
        {
            ls+=arr[i];
        }
        int currsum=ls;
        for(int i=1;i<=k;i++)
        {
            currsum=currsum-arr[k-i]+arr[n-i];
            maxs=Math.max(maxs,currsum);
        }
        maxs=Math.max(maxs,ls);
        System.out.print(maxs);
    }
}
