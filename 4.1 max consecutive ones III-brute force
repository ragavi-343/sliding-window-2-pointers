import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] arr=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        int k=sc.nextInt();
        int maxlen=0;
        for(int i=0;i<n;i++)
        {
            int zc=0;
            for(int j=i;j<n;j++)
            {
                if(arr[j]==0)
                {
                    zc++;
                }
                if(zc>k) break;
                maxlen=Math.max(maxlen,j-i+1);
            }
        }
        System.out.print(maxlen);
    }
}
