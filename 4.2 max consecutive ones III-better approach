import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] arr=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        int k=sc.nextInt();
        int maxlen=0,l=0,r=0,zc=0,n=nums.length;
        while(r<n)
        {
            if(nums[r]==0)
            {
                zc++;
            }
            while(zc>k)
            {
                if(nums[l]==0)
                {
                    zc--;
                }
                l++;
            }
            maxlen=Math.max(maxlen,r-l+1);
            r++;
        }
        System.out.print(maxlen);
    }
}
