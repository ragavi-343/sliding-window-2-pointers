import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] arr=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        int s=sc.nextInt();
        int maxcount=0;
        for(int i=0;i<n-1;i++)
        {
            int sum=0,c=0;
            for(int j=i;j<n;j++)
            {
                sum+=arr[j];
                if(sum<=s)
                {
                    c++;
                }
                else{
                    break;
                }
            }
            maxcount=Math.max(maxcount,c);
        }
        System.out.print(maxcount);
    }
}
